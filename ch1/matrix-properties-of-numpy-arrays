import numpy as np

mat = np.array([[1, 2], [3, 4]])
mat.transpose()
# array([[1, 3],
         [2, 4]])
mat.T
# array([[1, 3],
         [2, 4]])


A = np.array([[1, 2], [3, 4]])
B = np.array([[-1, 1], [0, 1]])
A @ B
# array([[-1, 3],
         [-3, 7]])
A * B
# array([[-1, 2],
         [ 0, 4]])

mat.trace()  # 5


from numpy import linalg
linalg.det(mat)  # -2.0000000000000004
linalg.inv(mat)
# array([[-2. , 1. ],
         [ 1.5, -0.5]])



